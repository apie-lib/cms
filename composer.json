{
  "name": "apie/cms",
  "description": "Composer package of the apie library: cms",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\Cms\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\Cms\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "apie/common": "self.version",
    "apie/core": "self.version",
    "apie/html-builders": "self.version",
    "nyholm/psr7": "^1.5"
  },
  "require-dev": {
    "apie/fixtures": "self.version",
    "apie/service-provider-generator": "0.11.2",
    "illuminate/support": "*",
    "phpunit/phpunit": "^9.5",
    "phpspec/prophecy-phpunit": "^2.0",
    "symfony/event-dispatcher": "^6.4"
  },
  "suggest": {
    "apie/cms-api-dropdown-option": "for having dropdowns for relations"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}